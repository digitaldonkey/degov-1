/**
 * Implements hook_theme().
 */
function degov_paragraph_text_theme($existing, $type, $theme, $path) {
  return [
    'paragraph__text' => [
      'template' => 'paragraph--text',
      'base hook' => 'paragraph',
    ],
    'paragraph__text__preview' => [
      'template' => 'paragraph--text--preview',
      'base hook' => 'paragraph',
    ],
  ];
}
