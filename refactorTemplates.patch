--- /dev/null
+++ b/modules/degov_paragraph_slideshow/css/include/_base--slick-navi.scss
@@ -0,0 +1,150 @@
+.l-slick-navi {
+  position: relative;
+  height: 75px;
+  width: 100%;
+  max-width: 100%;
+  margin: 0;
+  left: unset;
+  display: none;
+
+  .js & {
+    display: flex;
+  }
+}
+
+
+.slick-dots {
+  position: static;
+  bottom: auto;
+  width: auto;
+  flex-grow: 1;
+  flex-shrink: 1;
+  display: flex;
+  align-items: center;
+  justify-content: center;
+  flex-grow: 10;
+
+  line-height: 75px;
+  overflow-y: hidden;
+  overflow-x: auto;
+  scroll-behavior: smooth;
+  -ms-overflow-style: -ms-autohiding-scrollbar;
+
+  &::-webkit-scrollbar {
+    width: 3px;
+    display: none;
+  }
+  scrollbar-width: thin;
+
+  li {
+    height: 11px;
+    width: 11px;
+    min-width: 11px; // Fix ie11.
+    margin: 0 2px;
+    vertical-align: middle;
+    border-radius: 100%;
+    button {
+      height: 11px;
+      width: 11px;
+      padding: 0;
+      border-radius: inherit;
+      background-color: transparent;
+      border: 2px solid $peripheral-color-secondary;
+
+      &::before {
+        display: none;
+      }
+    }
+    &.slick-active {
+      button {
+        background-color: $peripheral-color-secondary;
+      }
+    }
+  }
+}
+
+.slick-controls {
+  display: none;
+  .slick-initialized + .l-slick-navi & {
+    order: 98;
+    display: flex;
+    flex-shrink: 1;
+    width: auto;
+    justify-content: flex-start;
+    color: $peripheral-color-secondary;
+    padding-left: 5px;
+  }
+
+  .slick__play {
+    //@include icon-before($fa-var-play);
+  }
+  .slick__pause {
+    //@include icon-before($fa-var-pause);
+  }
+
+  .slick__pause,
+  .slick__play {
+
+    // hide-text();
+    font: 0/0 a;
+    color: transparent;
+    text-shadow: none;
+    background-color: transparent;
+    border: 0;
+    //@include hide-text();
+
+    height: 75px;
+    padding: 10px 20px;
+    color: $peripheral-color-secondary;
+
+    &::before {
+      font-size: 22px;
+      text-shadow: none;
+      line-height: 55px;
+    }
+  }
+
+  .slick__play:before {
+    content: '►';
+  }
+  .slick__pause:before {
+    content: 'ǁ';
+    font-weight: bold;
+  }
+}
+
+.slick-arrow {
+  height: 75px;
+  width: 75px;
+  min-width: 75px; // Fix ie11.
+
+  position: static;
+  transform: none;
+  color: $peripheral-color-secondary;
+
+  &.slick-prev,
+  &.slick-next {
+    &:hover,
+    &:focus {
+      color: $peripheral-color-secondary;
+    }
+  }
+
+  &:before {
+    font-size: 34px;
+    width: 100%;
+    margin: 0 !important;
+    opacity: 1;
+    color: inherit;
+    top: 0;
+    padding: 0 10px;
+  }
+
+  &.slick-prev {
+    //@include icon-before($fa-var-angle-left);
+  }
+  &.slick-next {
+    //@include icon-before($fa-var-angle-right);
+    order: 999;
+  }
+}
