+.view-mode-slideshow {
+  display: flex;
+
+  // Reset and set the font size in the Slick slides to work around a known issue with display: inline-block
+  // https://github.com/kenwheeler/slick/issues/3581
+  font-size: $font-size-base;
+
+  .l-slide {
+    width: 100%;
+
+    .l-slide-media {
+      position: relative;
+      display: block; // this is a link.
+      &:hover {
+        text-decoration: none;
+      }
+
+      .no-image {
+        //@include icon-before($fa-var-image);
+        height: calc(100vw / 16 * 9);
+        //color: $gray-light;
+        //background: $gray-dark;
+        text-align: center;
+
+        display: flex;
+        justify-content: center;
+        align-items: center;
+
+        &:before {
+          font-size: 26vw;
+          margin: 10px;
+        }
+
+        @media (min-width: $screen-lg) {
+          &:before {
+            font-size: 440px;
+          }
+        }
+      }
+
+    }
+    .l-slide-text {
+      position: relative;
+
+      .slideshow-content-type-label,
+      h1, h2, h3, h4, h5, h6 {
+        font-weight: bold;
+      }
+
+      .slideshow-content {
+        @media (min-width: $screen-sm-max) {
+          position: absolute;
+          top: 20px;
+          left: 20px;
+          right: 20px;
+          bottom: 20px;
+          width: calc(100% - 40px);
+        }
+      }
+    }
+  }
+}
diff --git a/modules/degov_paragraph_slideshow/css/include/_slideshow-type-1.scss b/modules/degov_paragraph_slideshow/css/include/_slideshow-type-1.scss
new file mode 100644
index 000000000..20b1d1bb3
--- /dev/null
+++ b/modules/degov_paragraph_slideshow/css/include/_slideshow-type-1.scss
@@ -0,0 +1,138 @@
+.slideshow-type-1 {
+  position: relative;
+
+  .slick-dotted {
+    margin: 0;
+  }
+
+  // Improve loading behavior.g
+  .js .slideshow__slides:not(.slick-initialized) {
+    .slide:not(:first-child) {
+      display: none;
+    }
+  }
+
+  .l-slide {
+    padding-bottom: 75px;
+
+    @media (min-width: $screen-sm-max) {
+      display: flex;
+      flex-direction: row;
+      border: 0;
+      padding-bottom: 0;
+    }
+
+    .l-slide-media {
+      @media (min-width: $screen-sm-max) {
+        width: 65%;
+        padding-top: 36.5%; // Keep ratio.
+        height: 0;
+
+        .media--view-mode-slider_main_16_9 {
+          position: absolute;
+          top: 0;
+          left: 0;
+          bottom: 0;
+          width: 100%;
+          height: 100%;
+        }
+        .no-image {
+          height: calc(65vw / 16 * 9);
+          position: absolute;
+          top: 0;
+          bottom: 0;
+          left: 0;
+          right: 0;
+        }
+      }
+      @media (min-width: $screen-lg) {
+        width: 75%;
+        padding-top: 42%; // Keep ratio.
+        .no-image {
+          height: calc(1280px / 16 * 9 * .75 );
+        }
+      }
+    }
+  }
+
+  .l-slide-text {
+    position: relative;
+    padding: 22px 25px 0;
+    justify-content: center;
+    .slide__text__sub {
+      padding: 20px;
+    }
+
+    // A single slide is not "slick-initialized"
+    // -> no space for the .l-slick-navi required.
+    // This approach respects a sidbar if there is one.
+    @media (min-width: $screen-sm-max) {
+      height: 0;
+      overflow-x: auto;
+      width: 35%;
+      padding-top: 35%;
+
+      .slide__text__sub {
+        position: absolute;
+        top: 0;
+        left: 0;
+        bottom: 0;
+      }
+    }
+    @media (min-width: $screen-lg) {
+      width: 25%;
+      padding-top: 42%;
+    }
+  }
+
+  .slick-initialized .l-slide-text {
+    // Keep space for .l-slick-navi.
+    @media (min-width: $screen-sm-max) {
+      padding-top: 29%;
+    }
+    @media (min-width: $screen-lg) {
+      padding-top: 35%;
+    }
+  }
+
+  .l-slick-navi {
+    position: absolute;
+    bottom: 0;
+    right: 0;
+    left: 0;
+
+    @media (min-width: $screen-sm-max) {
+      max-width: 35%;
+      left: auto;
+    }
+    @media (min-width: $screen-lg) {
+      max-width: 25%;
+    }
+  }
+
+  .page-has-sidebar & {
+    // Narrow down slick-navi.
+    .slick-controls {
+      .slick__pause,
+      .slick__play {
+        padding-left: 5px;
+        padding-right: 5px;
+        &::before {
+          font-size: 15px;
+        }
+      }
+    }
+    .slick-arrow {
+      &::before {
+        font-size: 60px;
+      }
+    }
+  }
+}
+
+// Space for "Copyright below Slider".
+.paragraph-slideshow-wrapper--type_1 {
+  &.has-copyright {
+    padding-bottom: 2 * $padding-inner-box;
+  }
+}
